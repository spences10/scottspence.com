    "@upstash/redis": "^1.31.3",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "10.4.19",
    "cspell": "^8.8.4",
    "daisyui": "4.12.2",
    "date-fns": "3.6.0",
    "eslint": "9.4.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "^2.39.3",
    "fathom-client": "3.7.0",
    "graphql": "16.8.1",
