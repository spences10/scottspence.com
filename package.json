		"@vitest/ui": "^2.1.4",
		"autoprefixer": "10.4.20",
		"cspell": "^8.15.7",
		"daisyui": "4.12.14",
		"date-fns": "4.1.0",
		"eslint": "9.14.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
