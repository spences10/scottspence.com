    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tailwindcss/typography": "0.5.13",
    "@testing-library/svelte": "^5.1.0",
    "@types/marked": "^6.0.0",
    "@types/nodemailer": "^6.4.15",
